{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "cd ../ui && deno task build",
    "devPath": "http://localhost:8000",
    "distDir": "../ui"
  },
  "package": {
    "productName": "PsychoQuine",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "open": true
      },
      "clipboard": {
        "writeText": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "Copyright (c) 2026 Sxnnyside Project",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.sxnnyside.psychoquine",
      "longDescription": "Universal resource-agnostic Quine generator - A CoreRed experimental tool for meta-programming enthusiasts.",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Universal Quine Generator",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "PsychoQuine",
        "width": 1200,
        "minWidth": 800,
        "minHeight": 600,
        "decorations": true,
        "transparent": false,
        "center": true
      }
    ]
  }
}
